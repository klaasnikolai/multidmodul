public class Course {

	private  String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Course(String name) {
		super();
		this.name = name;
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Course c = new Course("4bhif");
		System.out.println(c.getName());

	}

}
